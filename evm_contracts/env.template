# Dynamic Configuration for Mesh Cross-Chain Swaps
# Copy this to .env and modify values as needed

# Private Keys
PRIVATE_KEY=your_private_key_here_without_0x_prefix

# Dynamic Exchange Rates (can be changed anytime)
ETH_TO_SUI_RATE=1013         # 1 ETH = 1013 SUI (default)
SUI_TO_ETH_RATE=0.00098        # 1 SUI = 0.00098 ETH (default)

# Dynamic Auction Configuration
AUCTION_START_RATE=6           # Starting rate multiplier (default: 6:1)
AUCTION_END_RATE=1             # Ending rate multiplier (default: 1:1)
AUCTION_DURATION=3600          # Auction duration in seconds (default: 1 hour)

# Dynamic Timelock Configuration
TIMELOCK_DURATION=3600         # Timelock duration in seconds (default: 1 hour)

# Dynamic Test Amounts
TEST_ETH_AMOUNT=0.01          # Test ETH amount (default: 0.01 ETH)
TEST_SUI_AMOUNT=10            # Test SUI amount (default: 10 SUI)

# Dynamic Order Configuration
ORDER_HASH=dynamic_order_hash  # Order hash (can be changed)

# RPC Configuration
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/your_infura_project_id
ETHERSCAN_API_KEY=your_etherscan_api_key_here

